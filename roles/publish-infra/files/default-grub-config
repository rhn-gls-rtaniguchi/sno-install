set default="0"

function load_video {
  insmod efi_gop
  insmod efi_uga
  insmod video_bochs
  insmod video_cirrus
  insmod all_video
}

load_video
set gfxpayload=keep
insmod gzio
insmod part_gpt
insmod ext2
rmmod tpm

set timeout=86400
### END /etc/grub.d/00_header ###

#search --no-floppy --set=root -l 'RHEL-7.4 Server.x86_64'

### Student and instructor fX and destroy generated by Makefile ###

menuentry 'EFI Firmware System Setup' $menuentry_id_option 'uefi-firmware' {
  fwsetup
}

menuentry 'Reboot' {
  reboot
}

menuentry 'Shutdown' {
  halt
}


