$TTL  1D
ocp4.example.com.	IN  SOA dns.ocp4.example.com. root.ocp4.example.com. (
							2019022400 ; serial
							3h	   ; refresh
							15	   ; retry
							1w	   ; expire
							3h	   ; minimum
)

@						IN 	NS 	dns.ocp4.example.com.

; BEGIN STATIC OCP4 ZONE RECORDS
dns					IN A 192.168.50.2
api					IN A 192.168.50.2
api-int				IN A 192.168.50.2
*.apps				IN A 192.168.50.2

; END STATIC OCP4 ZONE RECORDS

; BEGIN DYNAMIC OCP4 ZONE RECORDS
; END DYNAMIC OCP4 ZONE RECORDS
